	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Make iCal",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/index.ts",
			"outFiles": ["${workspaceFolder}/**/*.js"],
			"preLaunchTask": "tsc: build - tsconfig.json",
			"env": {
				"TEAM_NAME": "Pacific FC"
			}
		}
	]
